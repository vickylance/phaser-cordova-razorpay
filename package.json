{
  "name": "helloworld",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack && npm run cordova:dev",
    "cordova:dev": "cordova run browser ",
    "cordova:start": "cordova run browser -- --live-reload",
    "webpack": "webpack --mode development --info-verbosity verbose",
    "webpack:watch": "webpack --mode development --watch --info-verbosity verbose",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "com.razorpay.cordova": "1.4.5",
    "cordova-android": "^8.0.0",
    "cordova-browser": "^6.0.0",
    "cordova-electron": "^1.0.2",
    "cordova-ios": "^5.0.1",
    "cordova-osx": "^5.0.0",
    "cordova-plugin-browsersync": "^1.1.0",
    "phaser": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "cordova-plugin-whitelist": "^1.3.3",
    "file-loader": "^3.0.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "com.razorpay.cordova": {}
    },
    "platforms": [
      "android",
      "ios",
      "browser",
      "electron",
      "osx"
    ]
  }
}